<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <filter>
        <filter-name>allFilter</filter-name>
        <filter-class>filter.allFilter</filter-class>
    </filter>
    <filter-mapping>
        <filter-name>allFilter</filter-name>
        <url-pattern>/all.jsp</url-pattern>
    </filter-mapping>
    <servlet>
        <servlet-name>detailController</servlet-name>
        <servlet-class>controller.detailController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>nikeController</servlet-name>
        <servlet-class>controller.nikeController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>searchController</servlet-name>
        <servlet-class>controller.searchController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DetailController</servlet-name>
        <servlet-class>controller.DetailController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>TFController</servlet-name>
        <servlet-class>controller.typeController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>typeController</servlet-name>
        <servlet-class>controller.typeController</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>detailController</servlet-name>
        <url-pattern>/detailController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>nikeController</servlet-name>
        <url-pattern>/nikeController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>searchController</servlet-name>
        <url-pattern>/searchController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DetailController</servlet-name>
        <url-pattern>/DetailController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>TFController</servlet-name>
        <url-pattern>/TFController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>typeController</servlet-name>
        <url-pattern>/typeController</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>home.jsp</welcome-file>
    </welcome-file-list>
</web-app>
